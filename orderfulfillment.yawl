      </outputParam>
      <outputParam>
        <index>2</index>
        <name>ShipmentNotice</name>
        <type>ShipmentNoticeType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <defaultValue />
      </outputParam>
      <outputParam>
        <index>3</index>
        <name>TransportationQuote</name>
        <type>TransportationQuoteType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <defaultValue />
      </outputParam>
      <localVariable>
        <index>0</index>
        <name>ShipmentNotice</name>
        <type>ShipmentNoticeType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>0</index>
        <name>TransportationQuote</name>
        <type>TransportationQuoteType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>4</index>
        <name>Truckload</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>5</index>
        <name>DeliveryInstructions</name>
        <type>DeliveryInstructionsType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>&lt;ShipmentNumber/&gt;
 &lt;DeliveryDate&gt;2010-06-02&lt;/DeliveryDate&gt;
 &lt;DeliveryInstructions/&gt;
 &lt;DeliveryLocation/&gt;</initialValue>
      </localVariable>
      <localVariable>
        <index>6</index>
        <name>BillOfLading</name>
        <type>BillOfLadingType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>7</index>
        <name>TrailerUsage</name>
